CREATE TABLE `t_movie_personage` (
  `id` int NOT NULL AUTO_INCREMENT,
  `personage_code` varchar(255) COLLATE utf8mb4_general_ci DEFAULT NULL COMMENT '人物代码',
  `personage_name` varchar(255) COLLATE utf8mb4_general_ci DEFAULT NULL COMMENT '人物名称',
  `other_name` varchar(255) COLLATE utf8mb4_general_ci DEFAULT NULL COMMENT '其他名称',
  `personage_url` varchar(500) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci DEFAULT NULL COMMENT '人物链接',
  `avatar_url` varchar(500) COLLATE utf8mb4_general_ci DEFAULT NULL COMMENT '头像链接',
  `gender` varchar(20) COLLATE utf8mb4_general_ci DEFAULT NULL COMMENT '性别',
  `birth_date` date DEFAULT NULL COMMENT '出生日期',
  `birth_date_str` varchar(100) COLLATE utf8mb4_general_ci DEFAULT NULL COMMENT '出生日期字符串',
  `death_date` date DEFAULT NULL COMMENT '死亡日期',
  `death_date_str` varchar(100) COLLATE utf8mb4_general_ci DEFAULT NULL COMMENT '出生日期字符串',
  `birth_place` varchar(255) COLLATE utf8mb4_general_ci DEFAULT NULL COMMENT '出生地点',
  `alias` varchar(255) COLLATE utf8mb4_general_ci DEFAULT NULL COMMENT '别名',
  `family_members` varchar(500) COLLATE utf8mb4_general_ci DEFAULT NULL COMMENT '家庭成员',
  `profession` varchar(500) COLLATE utf8mb4_general_ci DEFAULT NULL COMMENT '职业',
  `collection_id` varchar(100) COLLATE utf8mb4_general_ci DEFAULT NULL COMMENT '收藏夹id',
  `movie_count` int DEFAULT NULL COMMENT '电影数量',
  `summary` text COLLATE utf8mb4_general_ci COMMENT '简介',
  `create_time` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建日期',
  `update_time` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT '更新日期',
  PRIMARY KEY (`id`),
  UNIQUE KEY `personage_code` (`personage_code`)
) ENGINE=InnoDB AUTO_INCREMENT=1265 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci COMMENT='电影人物表'